{
  "include": ["."],
  "compilerOptions": {
    "module": "ESNext",
    "lib": ["ES2019"],
    "importHelpers": true,
    "declaration": false,
    "sourceMap": false,
    "strict": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
